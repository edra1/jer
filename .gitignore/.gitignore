#include <iostream>
using namespace std;
int factorial(int n);

int main()
{
    int num=0, fact=0;
    cout<<"Calcular Factorial"<<endl;
    cout<<"Dame un numero: ";
    cin>>num;

    fact=factorial(num);
    cout<<"El factorial es: "<< fact<<endl;
    return 0;
}
int factorial(int n)
{
    if (n==0)
        n=1;
    else
        n*=factorial(n-1);
    return n;
}
